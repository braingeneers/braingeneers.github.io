{
  "env": {
    "browser": true,
    "commonjs": false,
    "es6": false,
    "jquery": true
  },
	"extends": "airbnb",
  "rules": {
    "no-console": 0,
    "func-names": 0,
    "object-shorthand": 0,
    "space-before-function-paren": 0,
    "prefer-template": 0,
    "quotes": ["error", "double"],
    "object-curly-spacing": [2, "never"]
  }
}
